{
  "name": "loveandpainkillers.com",
  "version": "0.3.0",
  "private": "true",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "servedev": "npx nodemon server.js",
    "lint-tooling": "npx eslint '*.js' 'tooling/**/*.js'",

    "build": "npm-run-all buildsite:*",
    "lint": "npm-run-all lint:*:* lint-tooling",
    "dev": "npm-run-all lint build watch",
    "watch": "npm-run-all --parallel watchsite:*",

    "buildsite:adammurray.name": "node tooling/clean.js adammurray.name && npm-run-all build:*:adammurray",
    "watchsite:adammurray.name": "npm-run-all --parallel watch:*:adammurray",
    "devsite:adammurray.name": "npm-run-all lint:*:adammurray buildsite:adammurray.name watchsite:adammurray.name",
    "lint:styles:adammurray": "npm-run-all prettify:adammurray stylelint:adammurray",
    "prettify:adammurray": "prettier './adammurray.name/assets/styles/**/*.scss' --write",
    "stylelint:adammurray": "stylelint './adammurray.name/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:images:adammurray": "node tooling/process-images.js adammurray.name",
    "build:styles:adammurray": "node tooling/build-css.js adammurray.name",
    "watch:images:adammurray": "onchange './adammurray.name/assets/images/**/*' -- npm run build:images:adammurray",
    "watch:styles:adammurray": "onchange './adammurray.name/assets/styles/**/*.scss' -- npm-run-all lint:styles:adammurray build:styles:adammurray",

    "buildsite:clivemurray.com": "node tooling/clean.js clivemurray.com && npm-run-all build:*:clivemurray",
    "watchsite:clivemurray.com": "npm-run-all --parallel watch:*:clivemurray",
    "devsite:clivemurray.com": "npm-run-all lint:*:clivemurray buildsite:clivemurray.com watchsite:clivemurray.com",
    "lint:styles:clivemurray": "npm-run-all prettify:clivemurray stylelint:clivemurray",
    "prettify:clivemurray": "prettier './clivemurray.com/assets/styles/**/*.scss' --write",
    "stylelint:clivemurray": "stylelint './clivemurray.com/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:copyassets:clivemurray": "node tooling/copy-assets.js clivemurray.com fonts",
    "build:images:clivemurray": "node tooling/process-images.js clivemurray.com",
    "build:styles:clivemurray": "node tooling/build-css.js clivemurray.com",
    "watch:assets:clivemurray": "onchange './clivemurray.com/assets/fonts' -- npm run build:copyassets:clivemurray",
    "watch:images:clivemurray": "onchange './clivemurray.com/assets/images/**/*' -- npm run build:images:clivemurray",
    "watch:styles:clivemurray": "onchange './clivemurray.com/assets/styles/**/*.scss' -- npm-run-all lint:styles:clivemurray build:styles:clivemurray",

    "buildsite:codenames.clivemurray.com": "node tooling/clean.js codenames.clivemurray.com && npm-run-all build:*:codenames",
    "watchsite:codenames.clivemurray.com": "npm-run-all --parallel watch:*:codenames",
    "devsite:codenames.clivemurray.com": "npm-run-all lint:*:codenames buildsite:codenames.clivemurray.com watchsite:codenames.clivemurray.com",
    "lint:js:codenames": "npx eslint codenames.clivemurray.com",
    "lint:styles:codenames": "npm-run-all prettify:codenames stylelint:codenames",
    "prettify:codenames": "prettier './codenames.clivemurray.com/assets/styles/style.scss' --write",
    "stylelint:codenames": "stylelint './codenames.clivemurray.com/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:copyassets:codenames": "node tooling/copy-assets.js codenames.clivemurray.com data,fonts",
    "build:styles:codenames": "node tooling/build-css.js codenames.clivemurray.com",
    "build:js:codenames": "node tooling/build-js.js codenames.clivemurray.com",
    "watch:assets:codenames": "onchange './codenames.clivemurray.com/assets/data' -- npm run build:copyassets:codenames",
    "watch:styles:codenames": "onchange './codenames.clivemurray.com/assets/styles/**/*.scss' -- npm-run-all lint:styles:codenames build:styles:codenames",
    "watch:js:codenames": "onchange './codenames.clivemurray.com/assets/js/**/*.js' -- npm-run-all lint:js:codenames build:js:codenames",

    "buildsite:cubi.st": "node tooling/clean.js cubi.st && npm-run-all build:*:cubist",
    "watchsite:cubi.st": "npm-run-all --parallel watch:*:cubist",
    "devsite:cubi.st": "npm-run-all lint:*:cubist buildsite:cubi.st watchsite:cubi.st",
    "lint:js:cubist": "npx eslint cubi.st",
    "lint:styles:cubist": "npm-run-all prettify:cubist stylelint:cubist",
    "prettify:cubist": "prettier './cubi.st/assets/styles/**/*.scss' --write",
    "stylelint:cubist": "stylelint './cubi.st/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:copyassets:cubist": "node tooling/copy-assets.js cubi.st fonts,sounds",
    "build:images:cubist": "node tooling/process-images.js cubi.st",
    "build:styles:cubist": "node tooling/build-css.js cubi.st",
    "build:js:cubist": "node tooling/build-js.js cubi.st",
    "watch:assets:cubist": "onchange './cubi.st/assets/fonts' './cubi.st/assets/sounds' -- npm run build:copyassets:cubist",
    "watch:images:cubist": "onchange './cubi.st/assets/images/**/*' -- npm run build:images:cubist",
    "watch:styles:cubist": "onchange './cubi.st/assets/styles/**/*.scss' -- npm-run-all lint:styles:cubist build:styles:cubist",
    "watch:js:cubist": "onchange './cubi.st/assets/**/*.js' -- npm-run-all lint:js:cubist build:js:cubist",

    "buildsite:hoodstock.rocks": "node tooling/clean.js hoodstock.rocks && npm-run-all build:*:hoodstock",
    "watchsite:hoodstock.rocks": "npm-run-all --parallel watch:*:hoodstock",
    "devsite:hoodstock.rocks": "npm-run-all lint:*:hoodstock buildsite:hoodstock.rocks watchsite:hoodstock.rocks",
    "lint:styles:hoodstock": "npm-run-all prettify:hoodstock stylelint:hoodstock",
    "prettify:hoodstock": "prettier './hoodstock.rocks/assets/styles/**/*.scss' --write",
    "stylelint:hoodstock": "stylelint './hoodstock.rocks/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:copyassets:hoodstock": "node tooling/copy-assets.js hoodstock.rocks fonts",
    "build:images:hoodstock": "node tooling/process-images.js hoodstock.rocks",
    "build:styles:hoodstock": "node tooling/build-css.js hoodstock.rocks",
    "watch:assets:hoodstock": "onchange './hoodstock.rocks/assets/fonts' -- npm run build:copyassets:hoodstock",
    "watch:images:hoodstock": "onchange './hoodstock.rocks/assets/images/**/*' -- npm run build:images:hoodstock",
    "watch:styles:hoodstock": "onchange './hoodstock.rocks/assets/styles/**/*.scss' -- npm-run-all lint:styles:hoodstock build:styles:hoodstock",

    "buildsite:idiotsplayground.com": "node tooling/clean.js idiotsplayground.com && npm-run-all build:*:idiotsplayground",
    "watchsite:idiotsplayground.com": "npm-run-all --parallel watch:*:idiotsplayground",
    "devsite:idiotsplayground.com": "npm-run-all lint:*:idiotsplayground  buildsite:idiotsplayground.com watchsite:idiotsplayground.com",
    "lint:styles:idiotsplayground": "npm-run-all prettify:idiotsplayground stylelint:idiotsplayground",
    "prettify:idiotsplayground": "prettier './idiotsplayground.com/assets/styles/**/*.scss' --write",
    "stylelint:idiotsplayground": "stylelint './idiotsplayground.com/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:styles:idiotsplayground": "node tooling/build-css.js idiotsplayground.com",
    "watch:styles:idiotsplayground": "onchange './idiotsplayground.com/assets/styles/**/*.scss' -- npm-run-all lint:styles:idiotsplayground build:styles:idiotsplayground",

    "buildsite:indigodown.com": "node tooling/clean.js indigodown.com && npm-run-all build:*:indigodown",
    "watchsite:indigodown.com": "npm-run-all --parallel watch:*:indigodown",
    "devsite:indigodown.com": "npm-run-all lint:*:indigodown buildsite:indigodown.com watchsite:indigodown.com",
    "lint:styles:indigodown": "npm-run-all prettify:indigodown stylelint:indigodown",
    "prettify:indigodown": "prettier './indigodown.com/assets/styles/**/*.scss' --write",
    "stylelint:indigodown": "stylelint './indigodown.com/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:copyassets:indigodown": "node tooling/copy-assets.js indigodown.com fonts,files",
    "build:images:indigodown": "node tooling/process-images.js indigodown.com",
    "build:styles:indigodown": "node tooling/build-css.js indigodown.com",
    "watch:assets:indigodown": "onchange './indigodown.com/assets/fonts' './indigodown.com/assets/files' -- npm run build:copyassets:indigodown",
    "watch:images:indigodown": "onchange './indigodown.com/assets/images/**/*' -- npm run build:images:indigodown",
    "watch:styles:indigodown": "onchange './indigodown.com/assets/styles/**/*.scss' -- npm-run-all lint:styles:indigodown build:styles:indigodown",

    "buildsite:loveandpainkillers.com": "node tooling/clean.js loveandpainkillers.com && npm-run-all build:*:loveandpainkillers",
    "watchsite:loveandpainkillers.com": "npm-run-all --parallel watch:*:loveandpainkillers",
    "devsite:loveandpainkillers.com": "npm-run-all lint:*:loveandpainkillers buildsite:loveandpainkillers.com watchsite:loveandpainkillers.com",
    "lint:js:loveandpainkillers": "npx eslint loveandpainkillers.com",
    "lint:styles:loveandpainkillers": "npm-run-all prettify:loveandpainkillers stylelint:loveandpainkillers",
    "prettify:loveandpainkillers": "prettier './loveandpainkillers.com/assets/styles/**/*.scss' --write",
    "stylelint:loveandpainkillers": "stylelint './loveandpainkillers.com/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:copyassets:loveandpainkillers": "node tooling/copy-assets.js loveandpainkillers.com data,fonts",
    "build:images:loveandpainkillers": "node tooling/process-images.js loveandpainkillers.com",
    "build:styles:loveandpainkillers": "node tooling/build-css.js loveandpainkillers.com",
    "build:js:loveandpainkillers": "node tooling/build-js.js loveandpainkillers.com",
    "watch:assets:loveandpainkillers": "onchange './loveandpainkillers.com/assets/fonts' './loveandpainkillers.com/assets/data' -- npm run build:copyassets:loveandpainkillers",
    "watch:images:loveandpainkillers": "onchange './loveandpainkillers.com/assets/images/**/*' -- npm run build:images:loveandpainkillers",
    "watch:styles:loveandpainkillers": "onchange './loveandpainkillers.com/assets/styles/**/*.scss' -- npm-run-all lint:styles:loveandpainkillers build:styles:loveandpainkillers",
    "watch:js:loveandpainkillers": "onchange './loveandpainkillers.com/assets/js/**/*.js' -- npm-run-all lint:js:loveandpainkillers build:js:loveandpainkillers",

    "buildsite:saffronmurray.com": "node tooling/clean.js saffronmurray.com && npm-run-all build:*:saffronmurray",
    "watchsite:saffronmurray.com": "npm-run-all --parallel watch:*:saffronmurray",
    "devsite:saffronmurray.com": "npm-run-all lint:*:saffronmurray buildsite:saffronmurray.com watchsite:saffronmurray.com",
    "lint:styles:saffronmurray": "npm-run-all prettify:saffronmurray stylelint:saffronmurray",
    "prettify:saffronmurray": "prettier './saffronmurray.com/assets/styles/**/*.scss' --write",
    "stylelint:saffronmurray": "stylelint './saffronmurray.com/assets/styles/**/*.scss' --customSyntax postcss-scss",
    "build:images:saffronmurray": "node tooling/process-images.js saffronmurray.com",
    "build:styles:saffronmurray": "node tooling/build-css.js saffronmurray.com",
    "watch:images:saffronmurray": "onchange './saffronmurray.com/assets/images/**/*' -- npm run build:images:saffronmurray",
    "watch:styles:saffronmurray": "onchange './saffronmurray.com/assets/styles/**/*.scss' -- npm-run-all lint:styles:saffronmurray build:styles:saffronmurray"
  },
  "author": "Clive Murray <clive+murray@clivemurray.com>",
  "license": "UNLICENSED",
  "type": "module",
  "browserslist": [
    ">0.3%, last 5 versions, not dead"
  ],
  "engines": {
    "node": "16.18.1",
    "npm": "^8"
  },
  "devDependencies": {
    "eslint": "7.17.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-unicorn": "28.0.2",
    "@springernature/eslint-config": "5.0.5",
    "nodemon": "2.0.20",
    "onchange": "7.1.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
